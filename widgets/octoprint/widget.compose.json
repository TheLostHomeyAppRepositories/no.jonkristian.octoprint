{
  "name": {
    "en": "OctoPrint Widget",
    "nl": "OctoPrint Widget",
    "da": "OctoPrint Widget",
    "de": "OctoPrint Widget",
    "es": "OctoPrint Widget",
    "fr": "OctoPrint Widget",
    "it": "OctoPrint Widget",
    "no": "OctoPrint Widget",
    "sv": "OctoPrint Widget",
    "pl": "OctoPrint Widget",
    "ru": "OctoPrint Widget",
    "ko": "OctoPrint Widget"
  },
  "height": 240,

  "api": {
    "getDeviceData": {
      "method": "GET",
      "path": "/device-data"
    },
    "listDevices": {
      "method": "GET",
      "path": "/list-devices"
    }
  },

  "settings": [
    {
      "type": "autocomplete",
      "id": "selectedDeviceId",
      "title": {
        "en": "Select your OctoPrint device"
      },
      "placeholder": {
        "en": "Start typing your device name..."
      }
    }
  ],
  "options": {
    "devices": {
      "source": "api",
      "endpoint": "listDevices",
      "map": {
        "value": "id",
        "label": "name"
      }
    }
  }
}
